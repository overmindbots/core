{
  "name": "@overmindbots/discord.js-command-manager",
  "version": "1.0.0",
  "description": "Commands manager for discord.js",
  "author": "Rafael Vidaurre <narzerus@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": true,
  "scripts": {
    "build": "npm run clean:build; tsc; npm run build-babel",
    "build-babel": "babel lib --out-dir lib --extensions '.js'",
    "clean:build": "rm -rf ./lib",
    "pre-publish": "npm run clean:build; npm run build; git add ./lib",
    "test": "jest",
    "pre-commit": "echo \"disabled\""
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "@overmindbots/shared-models": "^1.0.3",
    "@overmindbots/shared-utils/utils": "^1.0.3",
    "@types/node": "^9.0.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.42",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "discord.js": "^11.3.2",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3"
  }
}
