{
  "name": "@overmindbots/app-bots-web-panel-server",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=9.11.2"
  },
  "scripts": {
    "run-server": "babel-node --extensions \".ts\" src",
    "run-build-schema": "babel-node ./scripts/syncSchema.ts --extensions \".ts\"",
    "start-server": "nodemon -V -e ts --watch \"src\" --exec babel-node -- --extensions \".ts\" src",
    "start-schema": "nodemon -V -e ts --watch \"src\" --exec babel-node -- --extensions \".ts\" src/scripts/syncSchema",
    "lint": "tslint --project tsconfig.json",
    "production-build": "rm -rf ./build && babel ./src -d ./build --extensions \".ts\"",
    "production-build-schema": "node ./build/scripts/syncSchema.js",
    "production-start": "node ./build/index.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/runtime": "^7.0.0-beta.49",
    "@overmindbots/shared-models": "^1.0.0",
    "@overmindbots/shared-utils": "^1.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "btoa": "^1.1.2",
    "checksum": "^0.1.1",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "discord.js": "^11.3.2",
    "dotenv": "^5.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "form-data": "^2.3.2",
    "graphql": "0.13.0",
    "graphql-relay": "^0.5.5",
    "graphql-type-json": "^0.2.1",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.4",
    "morgan": "^1.9.0",
    "node-fetch": "^2.0.0",
    "passport": "^0.4.0",
    "passport-discord-oauth2": "^1.0.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-2": "^7.0.0-beta.49",
    "@babel/preset-stage-3": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@types/bluebird": "^3.5.20",
    "@types/checksum": "^0.1.30",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/graphql": "0.13.0",
    "@types/graphql-relay": "^0.4.5",
    "@types/graphql-type-json": "^0.1.2",
    "@types/lodash": "^4.14.105",
    "@types/mongoose": "^5.0.7",
    "@types/node": "^9.4.7",
    "@types/node-fetch": "^1.6.7",
