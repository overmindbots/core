{
  "name": "@overmindbots/service-bot-manager",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=9.11.2"
  },
  "author": "Rafael Vidaurre <narzerus@gmail.com>",
  "scripts": {
    "start": "npm run start-develop",
    "test": "echo \"no tests yet\"",
    "build": "npm run clean:build && babel src --out-dir build --extensions '.ts'",
    "clean:build": "rm -rf ./build",
    "start-production": "node ./build/index.js",
    "start-develop": "nodemon -e ts --watch src --exec \"babel-node ./src/index.ts --extensions '.ts'\"",
    "start-once": "babel-node ./src/index.ts --extensions '.ts'"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.53",
    "@overmindbots/shared-utils": "^1.0.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "kubernetes-client": "^5.3.1",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.6",
    "node-fetch": "^2.1.2",
    "tslint": "^5.10.0",
    "websocket": "^1.0.26",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/node": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.0.0-beta.53",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.108",
    "@types/mongoose": "^5.0.16",
    "@types/node": "^9.4.7",
    "@types/websocket": "^0.0.39",
    "@types/winston": "^2.3.9",
    "babel-plugin-module-resolver": "^3.1.0",
    "nodemon": "^1.17.4",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2"
  }
}
