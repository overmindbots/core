{
  "tslint.autoFixOnSave": [
    "ordered-imports",
    "member-access",
    "object-literal-sort-keys",
    "quotemark",
    "max-line-length"
  ],
  "tslint.packageManager": "yarn",
  "tslint.run": "onSave",
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.run": "onType",
  "search.exclude": {
    "**/node_modules": true,
    "node_modules": true
  },
}
