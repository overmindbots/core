{
  "lerna": "2.11.0",
  "packages": [
    "packages/core/*",
    "packages/shared/*",
    "packages/public/*"
  ],
  "version": "1.0.3",
  "changelog": {
    "repo": "bot-alchemy/monorepo-test",
    "labels": {
      "tag: new feature": ":rocket: New Feature",
      "tag: bug fix": ":bug: Bug Fix",
      "tag: enhancement": ":nail_care: Enhancement"
    }
  },
  "commands": {
    "publish": {
      "allowBranch": "master",
      "cdVersion": "prerelease",
      "conventionalCommits": true,
      "gitRemote": "upstream",
      "preid": "beta",
      "message": "chore(release): publish %s"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true
}
