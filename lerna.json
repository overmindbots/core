{
  "lerna": "2.11.0",
  "packages": [
    "packages/*"
  ],
  "commands": {
    "publish": {
      "allowBranch": ["development"],
      "conventionalCommits": true,
      "gitRemote": "origin",
      "preid": "beta",
      "message": "chore(release): publish %s"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "0.0.0"
}
